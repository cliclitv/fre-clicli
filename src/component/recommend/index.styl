@import "../../public/css/var.styl"

.recommend
  width 850px
  li
    width 20%
    display inline-block
    cursor: pointer
  .title
    width 150px
    height 40px
    overflow hidden
    margin: 10px 0
  .cover,.cover img
    width 150px
    height 210px
    box-shadow $shadow
    border-radius 2px
    object-fit cover